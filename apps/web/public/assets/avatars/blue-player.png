<svg width="512" height="512" viewBox="0 0 512 512" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3B82F6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#EF4444;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="gridGlow" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#60A5FA;stop-opacity:0.6" />
      <stop offset="100%" style="stop-color:#F87171;stop-opacity:0.6" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="512" height="512" rx="80" fill="url(#bg)"/>
  
  <!-- Grid pattern -->
  <g opacity="0.3">
    <line x1="128" y1="0" x2="128" y2="512" stroke="white" stroke-width="2"/>
    <line x1="256" y1="0" x2="256" y2="512" stroke="white" stroke-width="3"/>
    <line x1="384" y1="0" x2="384" y2="512" stroke="white" stroke-width="2"/>
    <line x1="0" y1="128" x2="512" y2="128" stroke="white" stroke-width="2"/>
    <line x1="0" y1="256" x2="512" y2="256" stroke="white" stroke-width="3"/>
    <line x1="0" y1="384" x2="512" y2="384" stroke="white" stroke-width="2"/>
  </g>
  
  <!-- Territory shapes -->
  <circle cx="150" cy="150" r="80" fill="#60A5FA" opacity="0.8"/>
  <circle cx="362" cy="362" r="80" fill="#F87171" opacity="0.8"/>
  
  <!-- Explosion/Impact effect in center -->
  <circle cx="256" cy="256" r="100" fill="url(#gridGlow)" opacity="0.6"/>
  <circle cx="256" cy="256" r="60" fill="white" opacity="0.8"/>
  <circle cx="256" cy="256" r="30" fill="#F9C74F" opacity="0.9"/>
  
  <!-- Sparkles/particles -->
  <circle cx="320" cy="200" r="8" fill="white" opacity="0.9"/>
  <circle cx="180" cy="320" r="6" fill="white" opacity="0.8"/>
  <circle cx="400" cy="280" r="7" fill="white" opacity="0.85"/>
  <circle cx="120" cy="220" r="6" fill="white" opacity="0.9"/>
</svg>
